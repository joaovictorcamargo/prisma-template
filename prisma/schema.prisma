// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Client {
  id       String @id @default(uuid())
  username String @unique
  password String

  @@map("client")
}

model Carrying {
  id                String      @id @default(uuid())
  typeOfPerson      String      
  cnpj              String @unique
  stateRegistration String
  socialReason      String
  fantasyName       String
  cep               String
  street            String
  number            String
  else              String
  neighborhood      String
  city              String
  state             String
   email            String @unique
   phone            String @unique
   Observation      String

  created_at        DateTime    @default(now())
  updated_at        DateTime    @updatedAt


  @@map("carryings")
}